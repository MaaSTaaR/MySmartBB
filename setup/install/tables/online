CREATE TABLE MySBB_online (id INT( 9 ) NOT NULL AUTO_INCREMENT PRIMARY KEY,username varchar(255) NOT NULL,path varchar(200) NOT NULL,logged varchar(30) NOT NULL,user_id int(9) NOT NULL,user_ip varchar(30) NOT NULL,hide_browse int(1) NOT NULL,username_style varchar(255) NOT NULL,user_location varchar(200) NOT NULL,subject_show int(1) NOT NULL,subject_id int(9) NOT NULL) TYPE = MYISAM AUTO_INCREMENT=1
