cp_style INT( 1 ) NOT NULL,admincp_toolbox INT( 1 ) NOT NULL,admincp_smile INT( 1 ) NOT NULL,admincp_icon INT( 1 ) NOT NULL,admincp_avater INT( 1 ) NOT NULL,group_order INT( 9 ) NOT NULL,admincp_contactus INT( 1 ) NOT NULL) TYPE = MYISAM AUTO_INCREMENT=1
